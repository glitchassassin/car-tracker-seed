{
  "name": "freedom-seed",
  "private": true,
  "type": "module",
  "prettier": "@epic-web/config/prettier",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "deploy": "npm run build && wrangler deploy",
    "dev": "react-router dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npm run cf-typegen",
    "preview": "npm run build && vite preview",
    "test:e2e": "playwright test --reporter line",
    "test:e2e:ci": "CI=true playwright test --reporter line",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "db:reset": "npx wrangler d1 migrations apply car-tracker-db --local",
    "db:seed": "node scripts/generate-mock-data.js 50 && node scripts/import-cars.js mock-cars.csv --clear",
    "db:reset-seed": "npm run db:reset && npm run db:seed"
  },
  "dependencies": {
    "@conform-to/react": "^1.9.0",
    "@conform-to/zod": "^1.9.0",
    "@epic-web/config": "^1.21.1",
    "csv-parse": "^6.1.0",
    "isbot": "^5.1.28",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.8.2",
    "remix-utils": "^8.8.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@cloudflare/vite-plugin": "^1.12.3",
    "@playwright/test": "^1.55.0",
    "@react-router/dev": "^7.8.2",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^22",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.26.0"
  }
}
